{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "sourceCsvData": {
    "baseUrl": "https://rekturacjazadanie.blob.core.windows.net/zadanie",
    "productFileOptions": {
      "fileName": "Products.csv",
      "hasHeader": true,
      "delimiter": ";"
    },
    "inventoryFileOptions": {
      "fileName": "Inventory.csv",
      "hasHeader": true,
      "delimiter": ","
    },
    "pricesFileOptions": {
      "fileName": "Prices.csv",
      "hasHeader": false,
      "delimiter": ","
    }
  }
}
